<!-- this tells aretemis what dmx stuff to send
Put it at Progam Files (x86)/Artemis/dat
file spec: http://artemiswiki.pbworks.com/w/page/68444887/Example%20DMX%20Declaration
all the cues: http://artemiswiki.pbworks.com/w/page/68387680/DMX%20Cues
example: https://raw.githubusercontent.com/KauyonKais/artemis-lighting/master/old_code/oldLight/DMXcommands.xml
-->
<?xml version="1.0" encoding="UTF-8"?>
<DMX_CONTROL version="2.00">
	<event continuous="yes" type="NORMAL_CONDITION_1">
		<timeblock mseconds="60">
			<setvalue index="0" value="255" change="0"/>
			<setvalue index="1" value="0" change="0"/>
			<setvalue index="2" value="100" change="0"/>
		</timeblock>
	</event>

	<event continuous="yes" type="HELM_IN_REVERSE">
		<timeblock mseconds="60">
			<setvalue index="0" value="42" change="0"/>
		</timeblock>
	</event>
</DMX_CONTROL>
